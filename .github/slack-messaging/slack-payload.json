{
    "blocks": [
        {
            "type": "section",
            "text": {
                "type": "plain_text",
                "text": ":gear: API suite result: ${{ job.status }}",
                "emoji": true
            }
        },
        {
            "type": "actions",
            "elements": [
                {
                    "type": "button",
                    "text": {
                        "type": "plain_text",
